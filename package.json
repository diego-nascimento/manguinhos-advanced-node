    "test:coverage": "yarn test -- --coverage",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^28.1.7",
    "@types/jsonwebtoken": "^8.5.9",
